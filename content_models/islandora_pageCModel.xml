    <foxml:datastream ID="DC" STATE="A" CONTROL_GROUP="X" VERSIONABLE="false">
        <foxml:datastreamVersion ID="DC1.0" LABEL="Dublin Core Record for this object" CREATED="2009-02-11T19:36:02.739Z" MIMETYPE="text/xml" FORMAT_URI="http://www.openarchives.org/OAI/2.0/oai_dc/">
            <foxml:xmlContent>
                <oai_dc:dc xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd">
                    <dc:title>Page Content Model</dc:title>
                    <dc:identifier>islandora:pageCModel</dc:identifier>
                </oai_dc:dc>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>
    <foxml:datastream ID="ISLANDORACM" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
        <foxml:datastreamVersion ID="ISLANDORACM.0" LABEL="Islandora Content Model" CREATED="2011-06-04T04:34:57.315Z" MIMETYPE="application/xml">
            <foxml:xmlContent>
                <content_model xmlns="http://www.islandora.ca" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="page" xsi:schemaLocation="http://www.islandora.ca http://www.islandora.ca/islandoracm.xsd">
                    <mimetypes>
                        <type>text/xml</type>
                        <type>image/tiff</type>
                        <type>image/tif</type>
                        <type>image/jp2</type>
                        <type>image/jpg</type>
                        <type>image/jpeg</type>
                        <type>text/plain</type>
                        <type>application/xml</type>
                    </mimetypes>
                    <ingest_rules></ingest_rules>
                    <datastreams>
                        <datastream dsid="TIFF"></datastream>
                        <datastream dsid="JPG"></datastream>
                        <datastream dsid="TN"></datastream>
                        <datastream dsid="HOCR"></datastream>
                        <datastream dsid="ENCODED_HOCR"></datastream>
                        <datastream dsid="OCR"></datastream>
                        <datastream dsid="DC"></datastream>
                        <datastream dsid="RELS-EXT"></datastream>
                        <datastream dsid="RELS-INT"></datastream>
                        <datastream dsid="TECHMD"></datastream>
                        <datastream dsid="MODS"></datastream>
                    </datastreams>
                </content_model>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>
    <foxml:datastream ID="DS-COMPOSITE-MODEL" STATE="A" CONTROL_GROUP="X" VERSIONABLE="false">
        <foxml:datastreamVersion ID="DS-COMPOSITE-MODEL.0" LABEL="Datastream Composite Model" CREATED="2012-05-29T22:36:02.182Z" MIMETYPE="text/xml" FORMAT_URI="info:fedora/fedora-system:FedoraDSCompositeModel-1.0">
            <foxml:xmlContent>
                <dsCompositeModel xmlns="info:fedora/fedora-system:def/dsCompositeModel#">
                    <dsTypeModel ID="DC">
                        <form FORMAT_URI="http://www.openarchives.org/OAI/2.0/oai_dc/" MIME="text/xml"/>
                    </dsTypeModel>
                    <dsTypeModel ID="RELS-EXT">
                        <form FORMAT_URI="info:fedora/fedora-system:FedoraRELSExt-1.0" MIME="text/xml"/>
                    </dsTypeModel>
                    <dsTypeModel ID="RELS-INT" optional="true">
                        <form FORMAT_URI="info:fedora/fedora-system:FedoraRELSInt-1.0" MIME="application/rdf+xml" />
                    </dsTypeModel>
                    <!-- should TIFF ds be called OBJ instead? Would that help with interoperability? -->
                    <dsTypeModel ID="TIFF">
                        <!-- This datastream contains the archival version of the page image -->
                        <form MIME="image/tiff" />
                        <form MIME="image/tif" />
                    </dsTypeModel>
                    <!-- do we want to rename this to TECHMD-FITS or something similar? -->
                    <dsTypeModel ID="TECHMD">
                        <!-- This datastream holds the technical metadata derived from the TIFF ds -->
                        <form MIME="text/xml" />
                    </dsTypeModel>
                    <dsTypeModel ID="MODS">
                        <!-- This datastream holds the descriptive metadata  -->
                        <form MIME="text/xml" />
                    </dsTypeModel>
                    <dsTypeModel ID="HOCR">
                        <!-- this datastream holds the OCR in an encoded format - from tesseract -->
                        <form MIME="text/xml" />
                    </dsTypeModel>
                    <dsTypeModel ID="ENCODED_OCR">
                        <!-- this datastream holds a transformed version of the HOCR and provides word coordinates for the page image - from xslt -->
                        <form MIME="text/xml" />
                    </dsTypeModel>
                    <dsTypeModel ID="OCR">
                        <!-- this datastream holds the plain text version of the HOCR - from xslt -->
                        <form MIME="text/plain" />
                    </dsTypeModel>
                    <dsTypeModel ID="JPEG">
                        <!-- this datastream holds a web version of the page image - from convert -->
                        <form MIME="image/jpg" />
                        <form MIME="image/jpeg" />
                    </dsTypeModel>
                    <dsTypeModel ID="TN">
                        <!-- this datastream holds a thumbnail version of the page image - from convert -->
                        <form MIME="image/jpg" />
                        <form MIME="image/jpeg" />
                        <form MIME="image/png" />
                    </dsTypeModel>                    
                </dsCompositeModel>
            </foxml:xmlContent>
        </foxml:datastreamVersion>
    </foxml:datastream>
</foxml:digitalObject>